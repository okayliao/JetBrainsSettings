<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'ServerApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'ServerApplication'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'Ctrl+Alt+L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'ServerApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'ServerApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>